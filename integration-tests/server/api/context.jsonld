{
    "@context": [
        "/api/hydra.jsonld",
        {
            "xsd": "http://www.w3.org/2001/XMLSchema#",
            "schema": "http://schema.org/",
            "rangeIncludes": { "@id": "schema:rangeIncludes", "@type": "@id" },
            "domainIncludes": { "@id": "schema:domainIncludes", "@type": "@id" },
            "examplevocab": "http://example.com/vocab#",
            "psd2": "http://example.com/psd2#",
            "Event": {
                "@id": "schema:Event",
                "@type": "@id"
            },
            "eventName": {
                "@id": "schema:name",
                "@type": "xsd:string"
            },
            "eventDescription": {
                "@id": "schema:description",
                "@type": "xsd:string"
            },
            "startDate": {
                "@id": "schema:startDate",
                "@type": "xsd:dateTime"
            },
            "endDate": {
                "@id": "schema:endDate",
                "@type": "xsd:dateTime"
            },
            "examplevocab:filter": {
                "@id": "examplevocab:filter",
                "@type": "@id"
            }
        }
    ]
}