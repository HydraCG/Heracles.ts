{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "quotemark": [true, "double"],
    "ter-indent": [true, 2],
    "no-extra-semi": true,
    "semicolon": [true, "always"],
    "trailing-comma": false
  },
  "jsRules": {
    "max-line-length": {
        "options": [120]
    }
}
}
